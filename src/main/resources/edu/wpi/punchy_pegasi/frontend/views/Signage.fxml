<BorderPane styleClass="signage-all" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.wpi.punchy_pegasi.frontend.controllers.SignageController">
    <top>
        <StackPane>
            <VBox fx:id="headerNormal">
                <HBox fx:id="signageHeader"/>
                <Separator/>
            </VBox>
        </StackPane>
    </top>
    <center>
        <HBox fx:id="signageBody">
            <VBox fx:id="signageBodyLeft"/>
        </HBox>
    </center>
    <right>
        <StackPane fx:id="signageBodyStackPane" style="-fx-padding: 50"/>
    </right>
</BorderPane>
